{
    "name": "__MSG_app_name__",
    "description": "__MSG_app_description__",
    "version": "0.0.41",
    "manifest_version": 2,
    "short_name": "__MSG_app_description__",
    "icons": {
        "16": "images/ins16.png",
        "128": "images/ins128.png"
    },
    "default_locale": "en",
    "content_scripts": [
        {
            "matches": [
                "http://mail.google.com/*",
                "https://mail.google.com/*"
            ],
            "js": [
                "assets/contentscript.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "assets/main.js"
    ]
}
